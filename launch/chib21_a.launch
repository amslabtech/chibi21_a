<?xml version="1.0"?>
<launch>
   <node pkg = "chibi21_a" name="astar" type="astar" output="screen" >
        <param name="hz" value="20" />
        <param name="wall_cost" value="1e+10" />
        <param name="wall_border" value="50" />
        <param name="wall_thickness" value="4" />
        <param name="landmark_set_param" value="1" />
        <param name="w" value="1.0" />
    </node>
    <node pkg = "chibi21_a" name= "map_manager" type= "map_manager" output= "screen">
        <param name="hz" value="10" />
        <param name="parallel_height" value="600" />
        <param name="parallel_width" value="900" />
        <param name="degree" value="170" />
    </node>
    <arg name="map_file" default="$(find chibi21_a)/map/Dmap1.yaml" />
    <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" output="screen" />

</launch>
